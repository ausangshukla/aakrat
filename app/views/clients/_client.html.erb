<tr id="<%= dom_id client %>">
  <td>
    <%= client.first_name %>
  </td>
  <td>
    <%= client.last_name %>
  </td>
  <td>
    <%= client.email %>
  </td>
  <td>
    <%= client.phone %>
  </td>
  <td>
    <%= client.user_id %>
  </td>
  <td>
    <%= client.company.name %>
  </td>
  <td>
    <%= link_to "Show" , client , {class: "btn btn-primary" }%>
  </td>
</tr>